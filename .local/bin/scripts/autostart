#!/bin/bash

xset r rate 500 25

function austart {
    ps -C "${2:-$1}" >/dev/null || $1 &
}

fehbg

austart rootblocks
austart "kmonad $HOME/.config/kmonad/def.kbd" kmonad
austart sxhkd
austart nm-applet
austart dunst
austart "picom --experimental-backends" picom
austart uim-xim
austart libinput-gestures
# austart "greenclip daemon" greenclip
austart "bash /usr/bin/clipmenud"
austart lxpolkit
austart xfce4-power-manager
# austart stretchly
austart "easyeffects --gapplication-service" easyeffects
austart redshift
austart telegram-desktop telegram-deskto
# austart discord Discord
austart vk
austart crow
austart flameshot

# tmux sessions
tmuxses sys
tmuxses main

exit 0
